<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <connectionStrings>
    <add name="SqlConnectionString" connectionString="{connectionString}" />
  </connectionStrings>
  <appSettings>
    <add key="DBType" value="{namespace}.DAL" />
  </appSettings>
  <system.web>
    <compilation debug="false" targetFramework="4.0" />
  </system.web>
</configuration>
